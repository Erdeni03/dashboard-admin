{
  "trailingComma": "none",
  "useTabs": true,
  "semi": false,
  "tabWidth": 1,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 80,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^components/(.*)$",
    "^views/(.*)$",
    "^providers/(.*)$",
    "^services/(.*)$",
    "^assets/(.*)$",
    "^types/(.*)$",
    "^hooks/(.*)$",
    "^store/(.*)$",
    "^utils/(.*)$",
    "^configs/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}